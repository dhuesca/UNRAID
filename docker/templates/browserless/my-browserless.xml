<?xml version="1.0"?>
<Container version="2">
  <Name>browserless</Name>
  <Repository>ghcr.io/browserless/chromium</Repository>
  <Registry>https://github.com/orgs/browserless/packages</Registry>
  <Network>nasty</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://docs.browserless.io/docker/docker-quickstart</Project>
  <Overview/>
  <Category/>
  <WebUI>http://[IP]:[PORT:3012]/docs</WebUI>
  <TemplateURL/>
  <Icon>https://github.com/dhuesca/UNRAID/raw/main/docker/templates/browserless/browserless.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled></DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="PORT" Target="3000" Default="3012" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false"></Config>
  <Config Name="ENABLE_DEBUGGER" Target="ENABLE_DEBUGGER" Default="false|true" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="CONCURRENT" Target="CONCURRENT" Default="16" Mode="" Description="MAX CONCURRENT SESSIONS" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="QUEUED" Target="QUEUED" Default="16" Mode="" Description="MAX QUEUE LENGTH" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="CORS" Target="CORS" Default="false|true" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="DATA_DIR" Target="DATA_DIR" Default="/tmp/browserless/data/" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="DATA_DIR PATH" Target="/tmp/browserless/data/" Default="/mnt/user/appdata/browserless/data/" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false"></Config>
  <Config Name="DOWNLOAD_DIR" Target="DOWNLOAD_DIR" Default="/tmp/browserless/downloads/" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="DOWNLOAD_DIR PATH" Target="/tmp/browserless/downloads/" Default="/mnt/user/appdata/browserless/downloads/" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false"></Config>
  <Config Name="ALLOW_GET" Target="ALLOW_GET" Default="false|true" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"></Config>
</Container>
