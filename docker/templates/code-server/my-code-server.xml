<?xml version="1.0"?>
<Container version="2">
  <Name>code-server</Name>
  <Repository>lscr.io/linuxserver/code-server</Repository>
  <Registry>https://github.com/orgs/linuxserver/packages/container/package/code-server</Registry>
  <Network>nasty</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://coder.com/docs/code-server</Project>
  <Overview/>
  <Category></Category>
  <WebUI>http://[IP]:[PORT:8443]</WebUI>
  <TemplateURL/>
  <Icon>https://github.com/dhuesca/UNRAID/raw/main/docker/templates/code-server/code-server.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled></DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="WEB UI" Target="8443" Default="8443" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false">8443</Config>
  <Config Name="CONFIG" Target="/config" Default="/mnt/user/appdata/code-server" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/code-server</Config>
  <Config Name="HOST ACCESS" Target="/host" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/</Config>
  <Config Name="PASSWORD" Target="PASSWORD" Default="" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="true"></Config>
  <Config Name="SUDO_PASSWORD" Target="SUDO_PASSWORD" Default="" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="true"></Config>
  <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false">99</Config>
  <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false">100</Config>
  <Config Name="UMASK" Target="UMASK" Default="022" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">022</Config>
</Container>
