<?xml version="1.0"?>
<Container version="2">
  <Name>npm</Name>
  <Repository>jc21/nginx-proxy-manager</Repository>
  <Registry>https://hub.docker.com/r/jc21/nginx-proxy-manager/</Registry>
  <Network>nasty</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://nginxproxymanager.com/guide</Project>
  <Overview/>
  <Category/>
  <WebUI>http://[IP]:[PORT:181]</WebUI>
  <TemplateURL/>
  <Icon>https://github.com/dhuesca/UNRAID/raw/main/docker/templates/npm/npm.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled></DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="WEB UI" Target="81" Default="181" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false"></Config>
  <Config Name="HTTP" Target="80" Default="180" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false"></Config>
  <Config Name="HTTPS" Target="443" Default="1443" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false"></Config>
  <Config Name="FTP" Target="21" Default="121" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false"></Config>
  <Config Name="SSH/SFTP" Target="2222" Default="1122" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false"></Config>
  <Config Name="DATA PATH" Target="/data/" Default="/mnt/user/appdata/npm/" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false"></Config>
  <Config Name="CERTIFICATES PATH" Target="/etc/letsencrypt/" Default="/mnt/user/appdata/npm/letsencrypt/" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false"></Config>
</Container>
