<?xml version="1.0"?>
<Container version="2">
  <Name>emulator-js</Name>
  <Repository>lscr.io/linuxserver/emulatorjs</Repository>
  <Registry>https://github.com/linuxserver/emulatorjs</Registry>
  <Network>nasty</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://emulatorjs.org/docs/</Project>
  <Overview/>
  <Category/>
  <WebUI>http://[IP]:[PORT:45110]</WebUI>
  <TemplateURL/>
  <Icon>https://github.com/dhuesca/UNRAID/raw/main/docker/templates/emulator-js/emulator-js.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled></DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="MGMT. WEB UI" Target="3000" Default="45110" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false"></Config>
  <Config Name="GAME WEB UI" Target="80" Default="45111" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false"></Config>
  <Config Name="CONFIG. PATH" Target="/config/" Default="/mnt/user/appdata/emulator-js/" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false"></Config>
  <Config Name="ROMS PATH" Target="/data/" Default="/mnt/user/appdata/emulator-js/data/" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false"></Config>
  <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="UMASK" Target="UMASK" Default="022" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false"></Config>
</Container>
