<?xml version="1.0"?>
<Container version="2">
  <Name>radarr</Name>
  <Repository>ghcr.io/hotio/radarr</Repository>
  <Registry>https://github.com/hotio/radarr/pkgs/container/radarr</Registry>
  <Network>nasty</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://wiki.servarr.com/radarr</Project>
  <Overview/>
  <Category/>
  <WebUI>http://[IP]:[PORT:7878]</WebUI>
  <TemplateURL/>
  <Icon>https://github.com/dhuesca/UNRAID/raw/main/docker/templates/radarr/radarr.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled></DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="WEB UI" Target="7878" Default="7878" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false"></Config>
  <Config Name="CONFIG. PATH" Target="/config/" Default="/mnt/user/appdata/radarr/" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false"></Config>
  <Config Name="HOST ACCESS" Target="/data/" Default="/mnt/user/data/" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false"></Config>
  <Config Name="R-CLONE MOUNT" Target="/mnt/user/debrid/torrents/" Default="/mnt/user/debrid/torrents/" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false"></Config>
  <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false"></Config>
  <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false"></Config>
  <Config Name="UMASK" Target="UMASK" Default="022" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false"></Config>
</Container>
