<?xml version="1.0"?>
<Container version="2">
  <Name>mysql8</Name>
  <Repository>mysql:8</Repository>
  <Registry>https://hub.docker.com/_/mysql</Registry>
  <Network>nasty</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://dev.mysql.com/doc/refman/8.0/en/server-administration.html</Project>
  <Overview>For a complete set of Environment Variables go to:&#xD;
&amp;gt;https://dev.mysql.com/doc/refman/8.0/en/server-system-variables.html</Overview>
  <Category/>
  <WebUI/>
  <TemplateURL>https://raw.githubusercontent.com/UNRA1DUser/unraid-docker-templates/master/templates/MySQL.xml</TemplateURL>
  <Icon>https://github.com/dhuesca/UNRAID/raw/main/docker/templates/mysql8/mysql8.png</Icon>
  <ExtraParams>--user=99:100</ExtraParams>
  <PostArgs>mysqld --log-error=/var/log/mysql/error.log</PostArgs>
  <CPUset/>
  <DateInstalled></DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="PORT" Target="3306" Default="3306" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false">3306</Config>
  <Config Name="CONFIG. PATH" Target="/etc/my.cnf" Default="/mnt/user/appdata/mysql8/my.cnf" Mode="rw" Description="Configuration options - https://dev.mysql.com/doc/refman/8.0/en/option-files.html" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/mysql8/my.cnf</Config>
  <Config Name="DATA PATH" Target="/var/lib/mysql" Default="/mnt/user/appdata/mysql8/data" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/mysql8/data</Config>
  <Config Name="LOGS PATH" Target="/var/log/mysql" Default="/mnt/user/appdata/mysql8/logs" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/mysql8/logs</Config>
  <Config Name="MYSQL_ROOT_PASSWORD" Target="MYSQL_ROOT_PASSWORD" Default="" Mode="" Description="Configure the 'root' (admin) password" Type="Variable" Display="always" Required="true" Mask="true"></Config>
</Container>
