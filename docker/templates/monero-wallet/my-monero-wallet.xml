<?xml version="1.0"?>
<Container version="2">
  <Name>monero-wallet</Name>
  <Repository>ich777/monero-gui</Repository>
  <Registry>https://hub.docker.com/r/ich777/monero-gui</Registry>
  <Network>nasty</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://www.getmonero.org/</Project>
  <Overview/>
  <Category/>
  <WebUI>http://[IP]:[PORT:6780]/vnc.html?autoconnect=true</WebUI>
  <TemplateURL/>
  <Icon>https://github.com/dhuesca/UNRAID/raw/main/docker/templates/monero-wallet/monero-wallet.png</Icon>
  <ExtraParams>--restart=unless-stopped</ExtraParams>
  <PostArgs/>
  <CPUset/>
  <DateInstalled></DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="WEB UI" Target="8080" Default="6780" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false"></Config>
  <Config Name="DATA PATH" Target="/monero/" Default="/mnt/cache/appdata/monero/" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false"></Config>
  <Config Name="UID" Target="UID" Default="" Mode="" Description="99" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="GID" Target="GID" Default="" Mode="" Description="100" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="UMASK" Target="UMASK" Default="" Mode="" Description="022" Type="Variable" Display="always" Required="true" Mask="false"></Config>
</Container>
