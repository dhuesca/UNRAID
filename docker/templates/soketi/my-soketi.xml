<?xml version="1.0"?>
<Container version="2">
  <Name>soketi</Name>
  <Repository>quay.io/soketi/soketi</Repository>
  <Registry>https://github.com/soketi/soketi</Registry>
  <Network>nasty</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://docs.soketi.app</Project>
  <Overview/>
  <Category/>
  <WebUI>http://[IP]:[PORT:6001]</WebUI>
