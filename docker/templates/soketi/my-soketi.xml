  <Config Name="SOKETI_ADAPTER_DRIVER" Target="SOKETI_ADAPTER_DRIVER" Default="redis|nats|local|cluster" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="SOKETI_DB_POOLING_ENABLED" Target="SOKETI_DB_POOLING_ENABLED" Default="true|false" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="SOKETI_DB_REDIS_HOST" Target="SOKETI_DB_REDIS_HOST" Default="valkey7" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="SOKETI_DB_REDIS_PORT" Target="SOKETI_DB_REDIS_PORT" Default="6379" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="SOKETI_DB_REDIS_USERNAME" Target="SOKETI_DB_REDIS_USERNAME" Default="soketi" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"></Config>
