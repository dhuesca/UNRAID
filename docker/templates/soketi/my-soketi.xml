  <Config Name="SOKETI_DB_POSTGRES_PASSWORD" Target="SOKETI_DB_POSTGRES_PASSWORD" Default="" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="true"></Config>
  <Config Name="SOKETI_DB_POSTGRES_DATABASE" Target="SOKETI_DB_POSTGRES_DATABASE" Default="soketi" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"></Config>
