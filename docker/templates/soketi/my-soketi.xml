  <Config Name="WEB UI" Target="6001" Default="6001" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false"></Config>
  <Config Name="SOKETI_DEBUG" Target="SOKETI_DEBUG" Default="1|0" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false">1</Config>
