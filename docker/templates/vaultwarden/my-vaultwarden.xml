<?xml version="1.0"?>
<Container version="2">
  <Name>vaultwarden</Name>
  <Repository>vaultwarden/server</Repository>
  <Registry>https://hub.docker.com/r/vaultwarden/server/</Registry>
  <Network>nasty</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://github.com/dani-garcia/vaultwarden/</Project>
  <Overview/>
  <Category/>
  <WebUI>http://[IP]:[PORT:80]/admin</WebUI>
  <TemplateURL/>
  <Icon>https://github.com/dhuesca/UNRAID/raw/main/docker/templates/vaultwarden/vaultwarden.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled></DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="WEB UI" Target="80" Default="4743" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false"></Config>
  <Config Name="CONFIG. PATH" Target="/data/" Default="/mnt/user/appdata/vaultwarden/" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false"></Config>
  <Config Name="ADMIN_TOKEN" Target="ADMIN_TOKEN" Default="" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="true"></Config>
</Container>
