<?xml version="1.0"?>
<Container version="2">
  <Name>adguard</Name>
  <Repository>adguard/adguardhome</Repository>
  <Registry>https://registry.hub.docker.com/r/adguard/adguardhome</Registry>
  <Network>br0</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project/>
  <Overview/>
  <Category/>
  <WebUI>http://[IP]</WebUI>
  <TemplateURL/>
  <Icon>https://github.com/dhuesca/UNRAID/raw/main/docker/templates/adguard/adguard.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled></DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="CONFIG. PATH" Target="/opt/adguardhome/conf/" Default="/mnt/user/appdata/adguard/" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false"></Config>
  <Config Name="WORKING DIR PATH" Target="/opt/adguardhome/work/" Default="/mnt/user/appdata/adguard/work/" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false"></Config>
</Container>
