<?xml version="1.0"?>
<Container version="2">
  <Name>libreoffice</Name>
  <Repository>lscr.io/linuxserver/libreoffice</Repository>
  <Registry>https://github.com/orgs/linuxserver/packages/container/package/libreoffice</Registry>
  <Network>nasty</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://help.libreoffice.org/latest</Project>
  <Overview/>
  <Category/>
  <WebUI>http://[IP]:[PORT:2996]</WebUI>
  <TemplateURL/>
  <Icon>https://github.com/dhuesca/UNRAID/raw/main/docker/templates/libreoffice/libreoffice.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled></DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="WEB UI" Target="3000" Default="2996" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false"></Config>
  <Config Name="CONFIG. PATH" Target="/config/" Default="/mnt/user/appdata/libreoffice/" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false"></Config>
  <Config Name="HOST ACCESS" Target="/host/" Default="/mnt/user/data/" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false"></Config>
  <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="UMASK" Target="UMASK" Default="022" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"></Config>
</Container>
